    <!-- http://www.semanticweb.org/tvanl/ontologies/2020/8/benhlua#Bọ_Xích_Đen -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/tvanl/ontologies/2020/8/benhlua#Bọ_Xích_Đen">
        <rdf:type rdf:resource="http://www.semanticweb.org/tvanl/ontologies/2020/8/benhlua#Bệnh"/>
        <rdf:type rdf:resource="http://www.semanticweb.org/tvanl/ontologies/2020/8/benhlua#Sâu_Hại"/>
        <diseaseSeason rdf:resource="http://www.semanticweb.org/tvanl/ontologies/2020/8/benhlua#Hè_Thu"/>
        <diseaseStage rdf:resource="http://www.semanticweb.org/tvanl/ontologies/2020/8/benhlua#Giai_đoạn_làm_đồng"/>
